{
  "mcpServers": {
    "default-server": {
      "url": "http://127.0.0.1:8080/mcp",
      "disabled": false,
      "timeout": 60000
    },
    "another-server": {
      "command": "node",
      "args": ["build/index.js", "--debug"],
      "env": {
        "API_KEY": "your-api-key",
        "DEBUG": "true"
      }
    }
  }
}